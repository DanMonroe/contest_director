{{#paper-card}}
    {{#paper-card-content}}
        <h3>{{model.contest.name}}</h3>
        <h3>{{model.pilotclass.name}}</h3>
        <h3>{{model.round.firstObject.name}}</h3>

        {{#paper-list}}
{{#each model.registeredpilots as |registeredpilot|}}
    <p>
        {{registeredpilot.pilot.fullName}} - {{#link-to "pilotscores" model.contest.id model.pilotclass.id model.round.firstObject.id registeredpilot.id}}Scores{{/link-to}}
    </p>
{{/each}}
        {{/paper-list}}
    {{/paper-card-content}}
{{/paper-card}}
